// creating database
create Database DB1; 

// using craeted database
use DB1; 

// creating table
create table employee
(
id int primary key,
name varchar(30),
dob datetime,
email varchar(40)
);

// Inserting data into table
insert into employee(id, name, dob, email)
values(1901, 'Rajshree' , 1998-8-29, 'rajshree.29patel@gmail.com')

//updating table
select * from employee;
update employee
set name= 'Rajshree Patel'
where id = 1901;

//Read operation
Select * from employee;

//Delete operation
delete from employee
where id = 1901;

